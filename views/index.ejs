        <div class="container-fluid mb-5">
            <div class="row title-row">
                <div class="col-lg-8 col-sm-12 title-column d-flex">
                    <h1>Exceptional stay <br>
                        Extraordinary adventure.</h1>
                </div>
            </div>
            <div class="row image">
                <div class="col-lg-12 image-wrapper">
                    <img src="images/hero-image.jpg" />
                </div>
            </div>
        </div>
        <!-- -------------------------------ABOUT US---------------------------- -->
        <section class="container-fluid">
            <div class="row image-with-text justify-content-center mb-5">
                <div class="col-lg-6 col-md-12">
                    <img src="https://picsum.photos/600" alt="" style="width:100%">
                </div>
